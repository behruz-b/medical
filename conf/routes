# An example controller showing a sample home page
GET     /                                           @controllers.HomeController.index(language = "uz")
GET     /$lang<"uz|en|ru">                          @controllers.HomeController.index(lang: String)
GET     /admin/$lang<"uz|en|ru">                    @controllers.HomeController.adminLogin(lang: String)
GET     /addAnalysisResult/$lang<"uz|en|ru">        @controllers.HomeController.addAnalysisResult(lang: String)
POST    /admin                                      @controllers.HomeController.loginPost
POST    /createPatient                              @controllers.HomeController.createUser
GET     /patients                                   @controllers.HomeController.getPatients
GET     /logout                                     @controllers.HomeController.logout

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               @controllers.Assets.versioned(path="/public", file: Asset)
->      /webjars                                     webjars.Routes