# An example controller showing a sample home page
GET     /                                           @controllers.HomeController.index(language = "uz")
GET     /reg                                        @controllers.HomeController.index(language = "uz")

GET     /login                                      @controllers.HomeController.login(lang: String ?= "uz")

POST    /login                                      @controllers.HomeController.loginPost
GET     /logout                                     @controllers.HomeController.logout

GET     /doc                                        @controllers.HomeController.addAnalysisResult(language = "uz")
POST    /doc                                        @controllers.HomeController.upload

POST    /patient                                    @controllers.HomeController.createUser
GET     /patients                                   @controllers.HomeController.getPatients
GET     /analysis-result/:customerId                @controllers.HomeController.analysisResult(customerId)


POST    /stub-sms                                   @controllers.HomeController.stubSmsRequest

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               @controllers.Assets.versioned(path="/public", file: Asset)
->      /webjars                                     webjars.Routes